{
  "author": "Wes Rice",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@octokit/core": "^3.4.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vue/test-utils": "^1.2.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.0",
    "jest": "^27.1.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.3",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.2.4",
    "verdaccio": "^5.1.3"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "license": "MIT",
  "name": "jest-style-transformers",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/wesrice/jest-style-transformers.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci --force-local",
    "build": "lerna run build",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "commit": "cz",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run bootstrap",
    "postregistry:unpublish": "git checkout lerna.json **/package-lock.json",
    "registry:publish": "npx --userconfig=.registry/.npmrc lerna publish --no-git-tag-version --no-push --no-verify-access --registry http://localhost:4873 --legacy-auth dGVzdDp0ZXN0",
    "registry:start": "npx --userconfig=.registry/.npmrc verdaccio --config .registryrc.js",
    "registry:unpublish": "lerna run registry:unpublish",
    "reinstall": "npm run clean && npm install",
    "semantic-release": "semantic-release",
    "test": "lerna run test --stream --parallel",
    "prepare": "husky install"
  },
  "version": "0.0.0"
}
